{
    "semi": false,
    "overrides": [
      {
        "files": "*.sol",
        "options": {
          "bracketSpacing": false,
          "printWidth": 145,
          "tabWidth": 4,
          "useTabs": false,
          "singleQuote": false,
          "explicitTypes": "always"
        }
      },
      {
        "files": "*.ts",
        "options": {
          "printWidth": 120,
          "tabWidth": 4,
          "singleQuote": true,
          "trailingComma": "all"
        }
      },
      {
      "files": "*.json",
      "options": {
        "printWidth": 1,
        "tabWidth": 2,
        "bracketSpacing": true
      }
    }
    ]
  }